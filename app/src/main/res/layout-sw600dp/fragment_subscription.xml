<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:it.casinovenezia.casinodivenezia="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bluetilebg"
    tools:context="it.casinovenezia.casinodivenezia.Subscription">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="18+"
        android:id="@+id/diciottoetichetta"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:paddingLeft="2dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/diciottopiu"
        android:text="@string/link"
        android:textSize="12dp"
        android:textColor="@color/white"
        android:layout_toRightOf="@+id/diciottoetichetta"
        android:layout_alignBottom="@+id/diciottoetichetta"
        android:paddingRight="2dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true" />

    <it.casinovenezia.casinodivenezia.CustomMenuTitleView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mysubscription"
        android:id="@+id/mysubscription"
        android:layout_below="@+id/diciottoetichetta"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:textColor="@color/white"
        android:textSize="40sp"
        it.casinovenezia.casinodivenezia:typeface="GothamXLight.otf"/>
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView4"
        android:src="@drawable/loginsmallshadow"
        android:layout_below="@+id/mysubscription"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp" />

    <ToggleButton
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:text="New ToggleButton"
        android:id="@+id/toggleButton"
        android:background="@drawable/toggle_switch"
        android:textOff=""
        android:textOn=""

        android:layout_below="@+id/imageView4"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginTop="50dp" />

    <ToggleButton
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:text="New ToggleButton2"
        android:id="@+id/toggleButton2"
        android:background="@drawable/toggle_switch"
        android:textOff=""
        android:textOn=""
        android:layout_below="@+id/toggleButton"
        android:layout_alignLeft="@+id/toggleButton"
        android:layout_alignStart="@+id/toggleButton"
        android:layout_marginTop="40dp" />

    <ToggleButton
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:text="New ToggleButton2"
        android:id="@+id/toggleButton3"
        android:background="@drawable/toggle_switch"
        android:textOff=""
        android:textOn=""
        android:layout_below="@+id/toggleButton2"
        android:layout_alignLeft="@+id/toggleButton2"
        android:layout_alignStart="@+id/toggleButton2"
        android:layout_marginTop="40dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/eventsnew"
        android:id="@+id/textView5"
        android:layout_alignTop="@+id/toggleButton"
        android:layout_toRightOf="@+id/toggleButton"
        android:layout_above="@+id/toggleButton2"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4"
        android:textColor="@color/white"
        android:paddingLeft="10dp"
        android:textSize="25sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/slotnews"
        android:id="@+id/textView6"
        android:layout_alignTop="@+id/toggleButton2"
        android:layout_toRightOf="@+id/toggleButton2"
        android:layout_above="@+id/toggleButton3"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5"
        android:textColor="@color/white"
        android:paddingLeft="10dp"
        android:textSize="25sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/pokernews"
        android:id="@+id/textView7"
        android:layout_alignTop="@+id/toggleButton3"
        android:layout_toRightOf="@+id/toggleButton3"
        android:layout_alignBottom="@+id/toggleButton3"
        android:layout_alignRight="@+id/textView6"
        android:layout_alignEnd="@+id/textView6"
        android:textColor="@color/white"
        android:paddingLeft="10dp"
        android:textSize="25sp" />

</RelativeLayout>
